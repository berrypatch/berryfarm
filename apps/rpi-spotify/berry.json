{
    "name": "rpi-spotify",
    "description": "Runs a headless Spotify player that can be controlled remotely by any Spotify app.",
    "variables": [
        {
            "name": "SPOTIFY_NAME",
            "type": "string",
            "required": true,
            "default": "MySpeaker"
        },
        {
            "name": "DEVICE_NAME",
            "type": "string",
            "required": true,
            "default": "/dev/snd"
        }
    ]
}